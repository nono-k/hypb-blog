---
interface Props {
  featureId: string;
}

const { featureId } = Astro.props;
---

<baseline-status featureId={featureId} class="baseline-status" />

<style>
  .baseline-status {
    --baseline-status-color-outline: rgba(37, 37, 54, 0.3);
    padding: 1rem 1rem 0.5rem;
    margin-inline: auto;
  }
</style>

<script>
  import "baseline-status";
</script>